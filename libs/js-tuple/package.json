{
  "name": "js-tuple",
  "version": "0.4.2",
  "description": "High performance tuples for Map/Set indexing, NestedMap/NestedSet, and trees.",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./min": {
      "require": "./dist/min/index.js",
      "import": "./dist/min/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && tsup",
    "test": "jest --runInBand",
    "test:scope": "jest --runInBand --scope",
    "test:cov": "jest --runInBand --coverage",
    "test:unit": "NODE_ENV=test jest --runInBand --forceExit",
    "lint": "pnpm run lint:format && pnpm run lint:style",
    "lint:fix": "pnpm run lint:format:fix && pnpm run lint:style:fix",
    "lint:format": "prettier --check '**/{src,test}/**/*.ts'",
    "lint:format:fix": "prettier --write '**/{src,test}/**/*.ts'",
    "lint:style": "eslint",
    "lint:style:fix": "eslint --fix",
    "lint:staged": "lint-staged",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "tuple",
    "immutable",
    "map key",
    "map",
    "set",
    "nested",
    "array key",
    "value equality",
    "nested map",
    "nested set",
    "performance",
    "memoization",
    "deep key",
    "dfs",
    "bfs",
    "data structure",
    "tree",
    "graph",
    "traversal"
  ],
  "author": "Farenheith",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codibre/js-utils.git"
  },
  "bugs": {
    "url": "https://github.com/codibre/js-utils.git/issues"
  },
  "homepage": "https://github.com/codibre/js-utils/tree/main/libs/fast-class-stringify#readme",
  "devDependencies": {
    "bintrees": "^1.0.2",
    "js-tree": "^2.0.2",
    "tree-model": "^1.0.7"
  }
}

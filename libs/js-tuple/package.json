{
  "name": "js-tuple",
  "version": "0.2.4",
  "description": "High performance tuples for Map/Set indexing",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "test": "jest --runInBand",
    "test:scope": "jest --runInBand --scope",
    "test:cov": "jest --runInBand --coverage",
    "test:unit": "NODE_ENV=test jest --runInBand --forceExit",
    "lint": "pnpm run lint:format && pnpm run lint:style",
    "lint:fix": "pnpm run lint:format:fix && pnpm run lint:style:fix",
    "lint:format": "prettier --check '**/{src,test}/**/*.ts'",
    "lint:format:fix": "prettier --write '**/{src,test}/**/*.ts'",
    "lint:style": "eslint",
    "lint:style:fix": "eslint --fix",
    "lint:staged": "lint-staged",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "json",
    "stringify",
    "fast-json-stringify",
    "class",
    "typescript",
    "nestjs",
    "swagger",
    "serialization",
    "performance",
    "schema",
    "decorator"
  ],
  "author": "Farenheith",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codibre/js-tuple.git"
  },
  "bugs": {
    "url": "https://github.com/codibre/js-tuple.git/issues"
  },
  "homepage": "https://github.com/codibre/js-tuple/tree/main/libs/fast-class-stringify#readme"
}
